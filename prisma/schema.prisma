generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model TestResult {
  id             String   @id @default(uuid())
  name           String
  email          String
  age            Int
  gender         String
  answers        String   
  primaryProfile String
  secondaryProfile String
  profileScores  String 
  profileData    String 
  createdAt      DateTime @default(now())
  updatedAt      DateTime @updatedAt
}